
{
  "manifest_version": 2,
  "version": "1.0",
  "short_name": "OtterTab",
  "name": "OtterTab",
  "description": "Creates custom new tab for Chrome",
  "browser_action": {
    "default_popup": "index.html",
    "default_title": "OtterTab"
  },
  "permissions": [
    "tabs",
    "storage",
    "identity"
  ],
  "chrome_url_overrides": {
    "newtab": "index.html"
  }
}